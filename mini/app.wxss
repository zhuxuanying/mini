/*线*/
.xian{
  width:100%;
  height:3rpx;
  background:#999
}
/*灰色*/
.divider{
  height:15rpx;
  background: #f1f1f1;
  box-shadow: 0 1rpx 1rpx rgba(0,0,0,.1) inset

}
.cell{
    background-color:#fff;
    font-size: 14px;  
}
.cell-item{
    border-bottom: 1px solid #f1f1f1;
    padding:20rpx 30rpx;
    background-color:#fff;
}
.cell-item:last-child{
    border:none;
}
/* 箭头*/
.arrow{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.arrow::after{
  content:""; 
  width:7px;
  height:7px;
  border-right:1px solid #999;
  border-bottom:1px solid #999;
  transform: rotate(-45deg)
}

/*加载更多*/
.loadmore {
  color: #888;
  font-size: 16px;
  line-height: 100px;
  text-align: center;
}

.loadmore.loading::before {
  content: '';
  width: 40px;
  height: 40px;
  margin-top: -10px;
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.hidden{
  display:none;
}
.navigator-hover{
  background-color:#fff;
  color:#000;
}